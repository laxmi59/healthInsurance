<h1>Sign Up</h1>
  <%= form_with model: @emp, url: sign_up_path do |f| %>
    <p>
    <%= f.label 'email' %><br>
    <%= f.text_field :email %>
    </p>
    <p>
    <%= f.label 'password' %><br>
    <%= f.password_field :password %>
    </p>
    <p>
    <%= f.label 'password_confirmation' %><br>
    <%= f.password_field :password_confirmation %>
    </p>
    <p>
    <%= f.label 'role' %><br>
    <%= f.text_field :role_id %>
    </p>
    <p>
    <%= f.submit 'Sign Up' %>
    </p>
  <% end %>
